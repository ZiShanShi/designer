<?xml version="1.0" encoding="UTF-8" ?>
<radar remark="雷达图">
    <id></id>
    <zlevel>0</zlevel>
    <z>2</z>
    <center>['50%', '50%']</center>
    <radius remark="number, string, Array">75%</radius>
    <startAngle>90</startAngle>
    <name>
        <show>true</show>
        <formatter>$formatter</formatter>
        <color>#333333</color>
        <fontStyle>normal</fontStyle>
        <fontWeight>normal</fontWeight>
        <fontFamliy>Courier New</fontFamliy>
        <fontSize>12</fontSize>
        <lineHeight></lineHeight>

        <backgroundColor>transparent</backgroundColor>
        <borderColor>#ccc</borderColor>
        <borderWidth>0</borderWidth>
        <borderRadius>0</borderRadius>
        <padding>0</padding>
        <width></width>
        <height></height>

        <shadowColor></shadowColor>
        <shadowBlur></shadowBlur>
        <shadowOffsetX>0</shadowOffsetX>
        <shadowOffsetY>0</shadowOffsetY>
        <textBorderColor>transparent</textBorderColor>
        <textBorderWidth>0</textBorderWidth>
        <textShadowColor>transparent</textShadowColor>
        <textShadowBlur></textShadowBlur>
        <textShadowOffsetX>0</textShadowOffsetX>
        <textShadowOffsetY>0</textShadowOffsetY>
        <rich>$rich</rich>
    </name>
    <nameGap>15</nameGap>
    <splitNumber>5</splitNumber>
    <scale remark="是否是脱离 0 值比例">false</scale>
    <silent remark="坐标轴是否是静态无法交互。">false</silent>
    <triggerEvent>false</triggerEvent>

    <axisLine>
        <show>true</show>
        <onZero remark="X 轴或者 Y 轴的轴线是否在另一个轴的 0 刻度上，只有在另一个轴为数值轴且包含 0 刻度时有效。">true</onZero>
        <onZeroAxisIndex remark="当有双轴时，可以用这个属性手动指定，在哪个轴的 0 刻度上。"></onZeroAxisIndex>
        <symbol remark="轴线两边的箭头 ['none', 'arrow']">none</symbol>
        <symbolSzie>[10, 15]</symbolSzie>
        <symbolOffset>[0, 0]</symbolOffset>
        <lineStyle>
            <color>#333</color>
            <width>1</width>
            <type remark="solid,dashed,dotted">solid</type>
            <shadowColor></shadowColor>
            <shadowBlur></shadowBlur>
            <shadowOffsetX></shadowOffsetX>
            <shadowOffsetY></shadowOffsetY>
            <opacity type="图形透明度 0 时不绘制该图形。"></opacity>
        </lineStyle>
    </axisLine>

    <splitLine remark="坐标轴在 grid 区域中的分隔线。">
        <show>true</show>
        <interval  remark="坐标轴分隔线的显示间隔">auto</interval >
        <lineStyle>
            <color>#333</color>
            <type remark="solid,dashed,dotted">solid</type>
            <shadowColor></shadowColor>
            <shadowBlur></shadowBlur>
            <shadowOffsetX></shadowOffsetX>
            <shadowOffsetY></shadowOffsetY>
            <opacity type="图形透明度 0 时不绘制该图形。"></opacity>
        </lineStyle>
    </splitLine>

    <splitArea remark="坐标轴在 grid 区域中的分隔区域。">
        <show>true</show>
        <interval  remark="坐标轴分隔线的显示间隔">auto</interval >
        <areaStyle>
            <color>['rgba(250,250,250,0.3)','rgba(200,200,200,0.3)']</color>
            <shadowColor></shadowColor>
            <shadowBlur></shadowBlur>
            <shadowOffsetX>0</shadowOffsetX>
            <shadowOffsetY>0</shadowOffsetY>
            <opacity type="图形透明度 0 时不绘制该图形。"></opacity>
        </areaStyle>
    </splitArea>

    <indicator remark="雷达图的指示器" type="array">
        <object>
            <name></name>
            <max></max>
            <min></min>
            <color></color>
        </object>

    </indicator>
</radar>
    