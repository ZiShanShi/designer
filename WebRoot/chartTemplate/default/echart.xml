<?xml version="1.0" encoding="UTF-8"?>
<!--未完成  geo parallel parallelAxis singleAxis timeline graphic calendar aria-->
<option pathDefault="true">

    <title type="file" mandatory="true">
    </title>

    <legend type="file" mandatory="false">
    </legend>

    <grid type="file" mandatory="false">
    </grid>

    <xAxis type="file" parent="grid" mandatory="false" >
    </xAxis>

    <yAxis type="file" parent="grid" mandatory="false" >
    </yAxis>

    <polar type="file" mandatory="false" remark="极坐标系" >
    </polar>

    <radiusAxis type="file" parent="polar" mandatory="false"  remark="极坐标系的径向轴">
    </radiusAxis>

    <angleAxis type="file" parent="polar" mandatory="false" remark="极坐标系的角度轴">
    </angleAxis>

    <radar type="file" mandatory="false" remark="雷达图">
    </radar>

    <dataZoom mandatory="false"  type="file" remark="组件 用于区域缩放，从而能自由关注细节的数据信息">
    </dataZoom>

    <visualMap  type="file" mandatory="false"  remark="视觉映射组件">
    </visualMap>

    <tooltip type="file" mandatory="false" default="true" remark="提示框组件。" >
    </tooltip>

    <axisPointer type="file" mandatory="false" parent="xAxis|yAxis|radiusAxis|angleAxis" default="true" remark="坐标轴指示器">
    </axisPointer>

    <toolbox type="file" mandatory="false" default="true" remark="工具栏">
    </toolbox>

    <brush mandatory="false" type="file" remark="区域选择组件">
    </brush>

    <dataset mandatory="false" type="file" remark="ECharts 4 开始支持了 数据集（dataset）">
    </dataset>

    <series type="file" mandatory="true" default="true" remark="系列列表。每个系列通过 type 决定自己的图表类型" >
    </series>

    <textStyle mandatory="false" default="true" type="file">
    </textStyle>


    <animation default="true">true</animation>
    <animationThreshold default="true">2000</animationThreshold>
    <animationDuration default="true">1000</animationDuration>
    <animationEasing default="true">linear</animationEasing>
    <animationDelay default="true">0</animationDelay>
    <animationDurationUpdate default="true">300</animationDurationUpdate>
    <animationDelayUpdate default="true">0</animationDelayUpdate>
    <animationEasingUpdate default="true">cubicOut</animationEasingUpdate >
    <hoverLayerThreshold default="true">3000</hoverLayerThreshold>
    <blendMode default="true" remark="图形的混合模式">source-over</blendMode>
    <useUTC remark="是否使用 UTC 时间。">false</useUTC>

</option>
