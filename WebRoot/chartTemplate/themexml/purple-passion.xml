        <lineStyle>
            <normal>
                <width>1</width>
                <color>#aaaaaa</color>
            </normal>
        </lineStyle>
        <symbolSize>7</symbolSize>
        <symbol>circle</symbol>
        <smooth>true</smooth>
        <color>#9b8bba, #e098c7, #8fd3e8, #71669e, #cc70af, #7cb4cc</color>

        <label>
